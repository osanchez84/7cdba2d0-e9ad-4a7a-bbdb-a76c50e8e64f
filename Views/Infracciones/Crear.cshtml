											.Placeholder("Ingresa folio")
											.HtmlAttributes(new { style = "width: 100%", required = "required" })
											)
									</div>
								</div>
								<div class="form-group col-md-4">
									<div class="controlForm">
										<label>Oficial <b>(obligatorio):</b></label>
