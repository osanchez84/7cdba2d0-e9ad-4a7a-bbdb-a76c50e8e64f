		</text>
        }

        


        buscarPersonasFisicas();
    });

    function buscarPersonasFisicas() {

        $.ajax({
            url: '@Url.Action("ajax_BuscarPersonasFiscas")',
            type: 'POST',
            success: function (result) {
                $("#personasFisicasAcc").html(result);
            }
        });
    }

    $("#ddlMarcas").on("change", function () {
        var value = $(this).val();
        _set_combo_catalog("CatSubmarcasByFilter", value, "idSubmarca", "false", "divSubmarca", "");
        $('#idSubmarca').kendoDropDownList();

    });

    //$("#idSubmarca").on("change", function () {
    //    var value = $(this).val();
    //    console.log(value);
    //    $('#hdnIdSubmarca').val(value);

    //});

    $("#ddlCatTipoServicio").on("change", function () {
        var value = $(this).val();
        console.log(value);
        $('#hdnIdCatTipoPersona').val(value);

    });
function submitCreateMoral() {
    var validator = $("#frmCreateMoral").kendoValidator().data('kendoValidator');
    
    var idTelefono = $("#Persona_PersonaDireccion_telefono");
    var idTelefonoValue = idTelefono.val();
    
    if (idTelefonoValue === null || idTelefonoValue === "") {
        idTelefono.prop("disabled", true);
    }

    idTelefono.off('blur');


    $("#idCatTipoServicio").prop("disabled",true)


    if (validator.validate()) {
        var formData = $("#frmCreateMoral").serialize();

        $.ajax({
            url: '@Url.Action("ajax_CrearPersonaMoral")',
            type: 'POST',
            data: formData,
            success: function (result) {
                sitteg_success('Elemento guardado correctamente');
                window.setTimeout(function () {
                    document.getElementById("sec3").style.display = "none";
                    document.getElementById("PersonaMoralLocalizada").style.display = "block";
                    $("#PersonaMoralLocalizada").html(result);
                    $('#addMoral').modal('hide');
                    sitteg_success('Elemento guardado correctamente');
                }, 500);
            },
            error: function (xhr, status) {
                var errmsg = xhr.responseText;
                console.log(errmsg);
                sitteg_warning('Lo sentimos, ha ocurrido un error.');
            }
        });
    } else {
        idTelefono.prop("disabled", false);
        $("#idCatTipoServicio").prop("disabled",false)

        sitteg_info('Debe llenar los campos requeridos');
        return;
    }
}


    function submitSearchPersonaMoral() {
        var Mydata = $("#frmSearchPersonaMoral").serialize();
        console.log(Mydata);
        $.ajax({
            url: '@Url.Action("ajax_BuscarPersonaMoral")',
            type: 'POST',
            data: Mydata,
            success: function (result) {
                document.getElementById("sec3").style.display = "none";
                document.getElementById("PersonaMoralLocalizada").style.display = "block";
                $("#PersonaMoralLocalizada").html(result);
                var grid = $("#gridPersonasMoralesAC").data("kendoGrid");
                grid.refresh();
            }
        });
    }

    function submitCreatePersonaFisica() {
    var nombreValue = $("#Persona_nombreFisico").val();
    var RFCValue = $("#Persona_RFCFisico").val();
    var coloniaValue = $("#Persona_PersonaDireccion_coloniaFisico").val();           
    var calleValue = $("#Persona_PersonaDireccion_calleFisico").val();            
     var numeroValue = $("#Persona_PersonaDireccion_numeroFisico").val();
    var fechaNacimientoValue = $("#Persona_fechaNacimiento").data("kendoDatePicker").value();
     $('.error-container').empty();

    if (!nombreValue  || !coloniaValue || !calleValue || !numeroValue || !fechaNacimientoValue) {
        if (!nombreValue) {
            asignarMensajeError("errorContainerPersona_nombreFisico", "Este campo es obligatorio.");
        }

        if (!coloniaValue) {
            asignarMensajeError("errorContainerPersona_PersonaDireccion_coloniaFisico", "Este campo es obligatorio.");
        }
        if (!calleValue) {
            asignarMensajeError("errorContainerPersona_calleFisico", "Este campo es obligatorio.");
        }
        if (!numeroValue) {
            asignarMensajeError("errorContainerPersona_PersonaDireccion_numeroFisico", "Este campo es obligatorio.");
        }
        if (!fechaNacimientoValue) {
            asignarMensajeError("errorContainerPersona_fechaNacimiento", "Este campo es obligatorio.");
        }

        return;
    }

