
<div class="mainContainer bg-light">
    
    <section class="mx-md-3 mx-lg-4 mx-xl-5 bg-white boxShadow my-5 rounded">
        <div class="row align-items-center justify-content-between px-4 px-md-4 pt-3 pb-2">
            <div class="col-12 col-md-auto">
                <div class="row align-items-center justify-content-center justify-content-md-start">
                    <div class="col-auto">
                        <div class="border border-2 border-dark rounded-circle">
                            <h5 class="m-0 numberCard d-flex align-items-center justify-content-center">
                                <b>1</b>
                            </h5>
                        </div>
                    </div>
                    <div class="col-auto p-0">
                        <i class="icon-infracion h1 colorPrimary"></i>
                    </div>
                    <div class="col-auto my-3">
                        <h2 class="m-0 h3"><b>Información de infracción</b></h2>
                    </div>
                </div>
            </div>

        </div>
        <div class="row">
            <div class="col-12 ps-0">
                <div class="bg-light py-1"></div>
            </div>
        </div>
        <div class="row">
            <div class="col-12 my-4">
                <h5 class="px-4"><b>Capturar infracción</b></h5>
                <h6 class="px-4 text-muted">Registro de infracción en <b class="colorPrimary">Registro de Infracciones y Accidentes</b></h6>
            </div>
        </div>
        <form class="row px-4 pb-4 mb-4 align-items-end">
            <div class="col-12 col-md-6 col-lg-3">
                <div class="controlForm my-3">
                    @(Html.Kendo().TextBox()
                        .Name("folio")
                        .Label(l => l.Content("Folio <b>(obligatorio)</b>:"))
                        .Placeholder("Ingresa folio de infracción")
                        .HtmlAttributes(new { style = "width: 100%" })
                        )

                </div>
            </div>
            <div class="col-12 col-md-6 col-lg-9 d-flex align-items-center h-95px">
                <p class="text-muted m-0 mxW-16rem"><b class="text-dark">Folio completo</b> incluyendo los ceros a la izquierda - Ejemplo:<b class="text-dark">003301</b></p>
            </div>
            <div class="col-12 col-md-6 col-lg-3">
                <div class="controlForm mb-3">
                    <label>Oficial <b>(obligatorio)</b></label>
                    <span>
                        @(Html.Kendo().DropDownList()
                            .Name("Oficial")
                            .DataTextField("")
                            .DataValueField("")
                            .HtmlAttributes(new { style = "width:100%;" })
                            .Filter(FilterType.Contains)
                            .DataSource(source => source
                            .Read(read => read.Action("GetProducts", "Home"))
                            )
                            )
                    </span>
                </div>
            </div>
            <div class="col-12 col-md-6 col-lg-3">
                <div class="controlForm mb-3">
                    <label>Municipio <b>(obligatorio)</b></label>
                    <span>
                        @(Html.Kendo().DropDownList()
                            .Name("Municipio")
                            .DataTextField("")
                            .DataValueField("")
                            .HtmlAttributes(new { style = "width:100%;" })
                            .Filter(FilterType.Contains)
                            .DataSource(source => source
                            .Read(read => read.Action("GetProducts", "Home"))
                            )
                            )
                    </span>
                </div>
            </div>
            <div class="col-12 col-md-6 col-lg-3">
                <div class="controlForm my-3">
                    <label>Fecha de infracción <b>(obligatorio)</b></label>
                    <span>
                        @(Html.Kendo().DatePicker()
                            .Name("FechaInfracción")
                            .Culture("es-ES")
                            .HtmlAttributes(new { style = "width: 100%", title = "Ingresa la fecha de infracción" })
                            )
                    </span>
                </div>
            </div>
            <div class="col-12 col-md-6 col-lg-3">
                <div class="controlForm my-3">
                    <label>Hora de infracción <b>(obligatorio)</b></label>
                    <span>
                        @(Html.Kendo().TimePicker()
                            .Name("HoraInfracción")
                            .Value("10:00 AM")
                            .HtmlAttributes(new { style = "width: 100%", title = "Hora de infracción" })
                            .DateInput()
                            )
                    </span>
                </div>
            </div>
            <div class="col-12 my-5">
                <div class="bg-light pt-1 mx-1"></div>
            </div>
            <div class="col-12 col-md-6 col-lg-3">
                <div class="controlForm mb-3">
                    <label>Carretera <b>(obligatorio)</b></label>
                    <span>
                        @(Html.Kendo().DropDownList()
                            .Name("Carretera")
                            .DataTextField("")
                            .DataValueField("")
                            .HtmlAttributes(new { style = "width:100%;" })
                            .Filter(FilterType.Contains)
                            .DataSource(source => source
                            .Read(read => read.Action("GetProducts", "Home"))
                            )
                            )
                    </span>
                </div>
            </div>
            <div class="col-12 col-md-6 col-lg-3">
                <div class="controlForm mb-3">
                    <label>Tramo <b>(obligatorio)</b></label>
                    <span>
                        @(Html.Kendo().DropDownList()
                            .Name("Tramo")
                            .DataTextField("")
                            .DataValueField("")
                            .HtmlAttributes(new { style = "width:100%;" })
                            .Filter(FilterType.Contains)
                            .DataSource(source => source
                            .Read(read => read.Action("GetProducts", "Home"))
                            )
                            )
                    </span>
                </div>
            </div>
            <div class="col-12 col-md-6 col-lg-3">
                <div class="controlForm my-3">
                    @(Html.Kendo().TextBox()
                        .Name("manufactura")
                        .Label(l => l.Content("Kilómetro <b>(obligatorio)</b>:"))
                        .Placeholder("Ingresa el kilómetro de la infracción")
                        .HtmlAttributes(new { style = "width: 100%", type = "number" })
                        )

                </div>
            </div>


        </form>
    </section>

    <section class="mx-md-3 mx-lg-4 mx-xl-5 bg-white boxShadow my-5 rounded pb-1">
        <div class="row align-items-center justify-content-between px-4 px-md-4 pt-3 pb-2">
            <div class="col-12 col-md-auto">
                <div class="row align-items-center justify-content-center justify-content-md-start">
                    <div class="col-auto">
                        <div class="border border-2 border-dark rounded-circle">
                            <h5 class="m-0 numberCard d-flex align-items-center justify-content-center">
                                <b>2</b>
                            </h5>
                        </div>
                    </div>
                    <div class="col-auto p-0">
                        <i class="icon-car h1 colorPrimary"></i>
                    </div>
                    <div class="col-auto my-3">
                        <h2 class="m-0 h3"><b>Vehículo</b></h2>
                    </div>
                </div>
            </div>
            <div class="col-12 col-md-6 col-lg-7 col-xl-auto">
                <div class="row">
                    <div class="col-auto btnOutline my-2 mx-auto mx-xl-2 p-0">
                        @(Html.Kendo().Button()
                            .Name("editSearch")
                            .HtmlAttributes(new { @class = "px-5" })
                            .Content("<h6 class=\"m-0\"><i class=\"icon-edit me-2\"></i><b>Editar busqueda</b></h6>"))
                    </div>
                </div>
            </div>

        </div>
        <div class="row">
            <div class="col-12 ps-0">
                <div class="bg-light py-1"></div>
            </div>
        </div>
        <div class="row">
            <div class="col-12 my-4">
                <h5 class="px-4"><b>Datos del vehículo</b></h5>
                <h6 class="px-4 text-muted">Vehículo dado de alta en la infracción.</h6>
            </div>
        </div>
        <div class="row px-4 align-items-end">
            <div class="col-12 mt-4 controlForm">
                <label>Tipo de búsqueda:</label>
            </div>
            <div class="col-12 mb-4">
                <h5 class="m-0 colorPrimary"><i class="icon-placa h4"></i> <b>Placa del vehículo</b></h5>
            </div>
        </div>
        <div class="row justify-content-between mb-1">
            <div class="col-12 col-lg-6 mt-4 mb-2">
                <h5 class="px-4"><b>Listado de búsqueda</b></h5>
                <h6 class="px-4 text-muted">Selecciona un vehículo para continuar.</h6>
            </div>
            <div class="col-12 col-lg-auto text-center">
                <div class="btnOutline btnOutlinePrimary px-4 my-3">
                    <button data-bs-toggle="modal" data-bs-target="#addCar">
                        <h5 class="m-0"><i class="me-2 icon-addCar"></i><b>Agregar nuevo vehículo</b></h5>
                    </button>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-12 px-4 rounded mb-4">
                <kendo-grid name="gridVehiculo" height="450">
                    <datasource type="DataSourceTagHelperType.Custom" custom-type="odata" page-size="20">
                        <transport>
                            <read url="https://demos.telerik.com/kendo-ui/service/Northwind.svc/Orders" />
                        </transport>

                    </datasource>
                    <sortable enabled="true" />

                    <pageable button-count="5" refresh="true" page-sizes="new int[] { 5, 10, 20 }">
                    </pageable>
                    <columns>
                        <column width="75" selectable="true"
                                header-html-attributes='new Dictionary<string,object> { ["class"] = "checkbox-align" }'
                                html-attributes='new Dictionary<string,object> { ["class"] = "checkbox-align" }' />
                        <column field="OrderDate" title="Placa" width="150" />
                        <column field="OrderID" title="Serie" width="150" />
                        <column field="OrderID" title="Tarjeta" width="150" />
                        <column field="ShipName" title="Marca" width="150" />
                        <column field="ShipCity" title="Submarca" width="150" />
                        <column field="OrderDate" title="Modelo" width="150" />
                        <column field="OrderID" title="Propietario" width="150" />
                        <column field="OrderID" title="No. económico" width="150" />
                        <column field="ShipName" title="Entidad" width="150" />
                        <column template="<button data-bs-toggle=modal data-bs-target=\#editVehiculo class='w-100 btn'><h6 class='m-0 colorPrimary'><i class='icon-edit me-2'></i><b>Editar</b></h6></button>" width="120" />
                    </columns>
                </kendo-grid>
            </div>
        </div>
    </section>


