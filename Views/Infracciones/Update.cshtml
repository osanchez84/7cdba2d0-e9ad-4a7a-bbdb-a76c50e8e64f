                                <div class="col-12 col-md-8 mx-md-auto col-lg-5 ">
                                    <div class="btnOutline btnOutlinePrimary my-3">
                                        <button data-bs-toggle="modal" data-bs-target="#addCar">
                                            <h5 class="m-0"><i class="me-2 icon-addCar"></i><b>Agregar nuevo vehículo</b></h5>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="tab-pane tabCustomConteiner fade" id="serie-vehiculo" role="tabpanel" aria-labelledby="serie-vehiculo-tab">
                    <div class="row px-4 mt-4 pb-4 align-items-end">
                        <div class="col-12 col-md-6 col-lg-4">
                            <div class="controlForm my-3">
                                @(Html.Kendo().TextBox()
                                    .Name("serie")
                                    .Label(l => l.Content("Número de serie del vehículo:"))
                                    .Placeholder("Ingresa el número de serie del vehículo")
                                    .HtmlAttributes(new { style = "width: 100%" })
                                    )
                            </div>
                        </div>
                        <div class="col-12 col-md-6 col-lg-4 ">
                            <div class="controlButton my-3">
                                <div class="controlButton my-3">
                                    @(Html.Kendo().Button()
                                        .Name("buscarSerie")
                                        .HtmlAttributes(new { @class = "btnPrimary" })
                                        .Content("<h5 class=\"m-0\"><b>Buscar</b></h5>"))
                                </div>
                            </div>
                        </div>


                    </div>
                    <div class="row">
                        <div class="col-12">
                            <div class="tabCustomEnd"></div>
                        </div>
                    </div>
                    <div class="row px-4 py-5 align-items-end bg-light">
